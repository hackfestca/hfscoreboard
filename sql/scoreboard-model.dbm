        _playerIp := _playerIpStr::inet;

        -- Get team from userIp 
        SELECT id INTO _teamId FROM team WHERE _playerIp << net ORDER BY id DESC LIMIT 1;
        if NOT FOUND then
            raise exception 'Team not found for %',_playerIp;
        end if;

